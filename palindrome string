//reverse a string and check wheather it palindrome or not
#include<stdio.h>
#include<string.h>
int main()
{
	char str11[100],str22[100];
	int n=0,i=0;
	printf("enter a string\n");
	scanf("%s",str11);
	while(str11[i]!='\0')
	{
		n++;
		i++;
	}
	i=0;
	while(n>0)
	{
		str22[i]=str11[n-1];
		n--;                      
		i++;
	}
	puts(str22);
	i=0;
	n=0;
	while(str22[i]!='\0')
	{
		if(str11[i]!=str22[i])
		n=1;
		i++;
	}
	if(n==1)
	printf("the given string is not a palindrome");
	else
	printf("palindrome");
	return 0;
}
