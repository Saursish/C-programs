//count the numer of vowel and consonant
#include<stdio.h>
#include<string.h>
int main()
{
	char str[100],ch;
	int i,v=0,c=0;
	printf("enter a string\n");
	gets(str);
	for(i=0;str[i]!='\0';i++)
	{
		if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U')
		v++;
		else if(str[i]>='a'&&str[i]<='z'||str[i]>='A'&&str[i]<='Z')
		c++;
	}
	printf("the number of vowels and consonant is %d and %d",v,c);
	return 0;
}
