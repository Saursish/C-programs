
#include<stdio.h>
#include<math.h>
typedef struct 
{
	int date;
	int month;
	int year;
} date;
int main()
{
	int a[15];
	a[1]=31,a[2]=28,a[3]=31,a[4]=30,a[5]=31,a[6]=30,a[7]=31,a[8]=31,a[9]=30,a[10]=31,a[11]=30,a[12]=31;
	date d1,d2,d3;
	//taking input of the 1st date
	printf("enter the 1st date (dd/mm/yyyy)\n");
	scanf("%d%*c%d%*c%d",&d1.date,&d1.month,&d1.year);
//2nd date
	printf("enter the 2nd date(dd/mm/yyyy)\n");
	scanf("%d%*c%d%*c%d",&d2.date,&d2.month,&d2.year);
	//condition check
	if(d1.date==d2.date&&d1.month==d2.month&&d1.year==d2.year)
	printf("equal");
	
	else
	{
		if(d1.year>d2.year)
		{
			if(d1.year%4==0&&d1.year%100!=0||d1.year%400==0)
			{
				a[2]=29;
			}
			if(d1.date<d2.date)
			{
				d3.date=d1.date+a[d1.month]-d2.date;
				d1.month--;
			}
			else
			{
				d3.date=d1.date-d2.date;
			}
			if(d1.month<d2.month)
			{
				d3.month=d1.month+12-d2.month;
				d1.year--;
			}
			else
			{
				d3.month=d1.month-d2.month;
			}
			d3.year=d1.year-d2.year;
		}
		else if(d2.year>d1.year)
		{
			if(d2.year%4==0&&d2.year%100!=0||d2.year%400==0)
			{
				a[2]=29;
			}
			if(d2.date<d1.date)
			{
				d3.date=d2.date+a[d2.month]-d1.date;
				d2.month--;
			}
			else
			{
				d3.date=d2.date-d1.date;
			}
			if(d2.month<d1.month)
			{
				d3.month=d2.month+12-d1.month;
				d2.year--;
			}
			else
			{
				d3.month=d2.month-d1.month;
			}
			d3.year=d2.year-d1.year;
		}
		else
		{
			if(d1.month>d2.month)
			{
				if(d1.year%4==0&&d1.year%100!=0||d1.year%400==0)
				{
					a[2]=29;
				}
				if(d1.date<d2.date)
				{
					d3.date=d1.date+a[d1.month]-d2.date;
					d1.month--;
				}
				else
				d3.date=d1.date-d2.date;
				d3.month=d1.month-d2.month;
			}
			else if(d2.month>d1.month)
			{
				if(d2.year%4==0&&d2.year%100!=0||d2.year%400==0)
				{
					a[2]=29;
				}
				if(d2.date<d1.date)
				{
					d3.date=d2.date+a[d2.month]-d1.date;
					d2.month--;
				}
				else
				d3.date=d2.date-d1.date;
				d3.month=d2.month-d1.month;
			}
			else
			{
			d3.date=abs(d1.date-d2.date);
			d3.month=0;
			}
			d3.year=0;
		}
		printf("the difference is %d days %d months %d years",d3.date,d3.month,d3.year);
	}
	return 0;
}
